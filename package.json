{
  "name": "unistyle",
  "version": "0.1.4",
  "description": "Write modular and scalable CSS using the next version of ECMAScript.",
  "main": "src/index.js",
  "bin": {
    "unistyle": "bin/unistyle"
  },
  "scripts": {
    "lint": "xo",
    "pretest": "npm run -s lint",
    "test": "ava 'test/*-test.js'"
  },
  "keywords": [
    "css",
    "styles",
    "stylesheet",
    "ecmascript",
    "es6",
    "es2015",
    "es7",
    "es2016",
    "universal javascript",
    "isomorphic",
    "no-cascade",
    "scalable",
    "large scale"
  ],
  "author": "Joakim Carlstein <joakim@klei.se>",
  "repository": "joakimbeng/unistyle",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.2.0",
    "xo": "^0.8.0"
  },
  "xo": {
    "space": true,
    "ignores": [
      "test/fixtures/**",
      "examples/**"
    ]
  },
  "directories": {
    "test": "test",
    "example": "examples"
  },
  "dependencies": {
    "absurd": "^0.3.5",
    "arrify": "^1.0.0",
    "babel": "^5.8.21",
    "pixelify": "^1.0.0",
    "yargs": "^3.21.0"
  }
}
